def prime(a,b):

    c=0

    for j in range(a+1,b):

        if j<=1:

            continue

        for i in range(2, j):

            if (j% i) == 0:

                break

        else:

            c+= 1

    return c

q=int(input())

for i in range (q):

      c=0

      x=input().split()

      c=prime(int(x[0]),int(x[1]))

      print(c)



